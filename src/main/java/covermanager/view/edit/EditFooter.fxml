<?import covermanager.view.edit.EditFooter?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<fx:root xmlns:fx="http://javafx.com/fxml" type="EditFooter"
         spacing="5" alignment="CENTER">
    <padding>
        <Insets left="5" right="5" top="5" bottom="5"/>
    </padding>
    <Button fx:id="saveButton" text="Save" maxWidth="150" HBox.hgrow="ALWAYS"/>
    <Button fx:id="cancelButton" text="Cancel" maxWidth="150" HBox.hgrow="ALWAYS"/>
</fx:root>